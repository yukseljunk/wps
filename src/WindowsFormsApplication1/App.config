<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>

    <add key="MakeFirstImageAsFeature" value="True"/>
    <add key="MergeBlockSize" value="600"/>
    <add key="ThumbnailSize" value="150"/>
    <add key="ResizeImages" value="True"/>
    <add key="ResizeSize" value="750"/>
    <add key="UseFtp" value="True"/>
    <add key="UseCache" value="True"/>
    <add key="ShowMessageBoxes" value="False"/>
  </appSettings>
</configuration>